{
  "name": "rickym270.github.io",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "preinstall": "node scripts/check-node-version.js || exit 0",
    "pretest": "node scripts/check-node-version.js || exit 0",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:api": "playwright test --project=api",
    "test:e2e": "playwright test --project=chromium --project=firefox --project=webkit",
    "serve": "http-server -p 4321 -c-1 .",
    "locators:update": "node scripts/update-locators.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "http-server": "^14.1.1"
  }
}
